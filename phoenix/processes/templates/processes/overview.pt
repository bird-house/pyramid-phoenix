<metal:block use-macro="main_template">

    <div metal:fill-slot="content">
      <div class="container">
        <div class="row">
          <div class="col"> 
            <div class="card">
              <div class="card-header">Choose one of the Web Processing Services</div>
            </div>
          </div>
        </div>

        <!-- section wps services -->
        <div class="row">
          <div class="col">
            <div class="card-deck" tal:repeat="item items">
              <div class="card" style="max-width: 18rem;">
                <div class="card-body">
                  <h5 class="card-title">
                    <a href="${item.url}">${item.title}</a>
                  </h5>
                  <p class="card-text">${item.description}</p>
                </div>
              </div>
            </div>
          </div>
        </div> <!-- row -->

      </div>
    </div>

</metal:block>
