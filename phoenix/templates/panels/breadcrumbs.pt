<section class="breadcrumbs" tal:condition="len(breadcrumbs) > 1">
  <nav aria-label="breadcrumbs">
    <ol class="breadcrumb">
      <li class="breadcrumb-item"
          tal:repeat="item breadcrumbs">
        <a href="${item.route_path}">
          ${item.title}
        </a>
      </li>
    </ol>
  </nav>
</section>
